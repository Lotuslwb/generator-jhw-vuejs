{
  "name": "<%= projectName %>",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "./node_modules/.bin/webpack-dev-server --hot --config webpack.server.js",
    "build": "rm -rf build/*.js && rm -rf build/*.css && webpack --config webpack.production.js",
    "watch": "webpack --watch"
  },
  "dependencies": {
    <% if (includeRouter) { %>
    "vue-router": "^0.6.0", <% } %>
    <% if (includeResource) { %>
    "vue-resource": "^0.1.16", <% } %>
    "vue": "^0.12.14"
  },
  "devDependencies": {
    <% if (includeSass) { %>
    "sass-loader": "^2.0.1",
    "node-sass": "^3.3.3",
    <% } %>
    "vue-loader": "^3.0.3",
    "html-loader": "^0.3.0",
    "style-loader": "^0.12.3",
    "css-loader": "^0.18.0",
    "postcss-loader": "^0.6.0",
    "csswring": "^4.0.0",
    "autoprefixer": "^6.0.2",
    "babel-loader": "^5.3.2",
    "babel-core": "^5.8.24",
    "babel-runtime": "^5.8.20",
    "webpack": "^1.12.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "webpack-dev-server": "^1.10.1"
  }
}
